/* CSS Document */


* {
	font-family: "PT Sans", Arial, Helvetica, sans-serif !important;
}

#resume .name {
	border-top: 10px solid rgb(255, 181, 31);
	background: rgb(248, 248, 248);
	padding: 50px 0 0px 20px !important;
}

h2.section-header,#resume .name {
	color: rgb(56, 67, 84) !important;
	padding: 0 0 0 20px;
}

#resume .content {
	color: rgb(85, 85, 85);
}

#resume .name {
	margin: 0 !important;
	font-weight: bold;
}

#resume .contact {
	margin: 0;
	padding: 10px;
	text-align: left !important;
	padding: 0 0 75px 20px !important;
	background: rgb(248, 248, 248);
}

h2.section-header {
	text-align: left !important;
	font-weight: bold !important;
	float: left;
}

#resume .content {
	font-size: 13px; /*background:url(../images/linebg.jpg) 0 0 repeat-x;*/
	padding-top: 0 !important;
	text-align: left !important;
	padding-left: 0 !important;
	width: 600px !important;
}

.summary,.specialties,.skills,.positions,.educations,.honors,.interests
	{
	border-bottom: 1px solid rgb(56, 67, 84);
	padding-bottom: 20px;
	float: left !important;
}

#resume .position .company-name,#resume .education .school-name,#resume .certification .certification-name,#resume .patent .patent-title,#resume .publication .publication-title
	{
	font-size: 16px;
	font-weight: bold;
	color: rgb(30, 120, 180);
}
/*#resume .contact { border-top:5px solid rgb(30,120,180) ;}*/
#resume {
	padding: 0px !important;
}

#resume .picture {
	height: 120px;
	position: relative;
	top: 50px;
	right: 70px;
	background: rgb(255, 181, 31);
	float: right;
	position: absolute;
	top: 50px;
}

#resume .picture img {
	padding: 5px;
	width: 100px;
	height: 110px;
	border: 1px solid #rgb(56, 67, 84 ) !important;
}

.picture {
	display: block !important;
}
/*#resume img {  background:rgb(255,181,31); border: 1px solid rgb(56,67,84); height: 120px; padding: 5px; position: absolute;right: 150px; top:185px;width: 100px;}*/
@page {
	margin: 0.0in;
	-fs-flow-top: "header";
	-fs-flow-bottom: "footer";
	-fs-flow-left: "left";
	-fs-flow-right: "right";
	padding: 0em;
}

