      function invitaionList(act){
      var list="";
      
      for(var i=0;i<checkboxesCheckedcom.length;i++){
      list=list+checkboxesCheckedcom[i]+"<br>";
      
      }
	list=list+"<input type='button' value='close' id='false' onclick='invitaionList(this.id)'>";
if(act=='true'){
